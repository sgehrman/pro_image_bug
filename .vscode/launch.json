{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Dashboard Linux",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "linux"
      // "args": [
      //   "--local-engine",
      //   "host_debug_unopt",
      // ]
    },
    {
      "name": "Dashboard Mac",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "macos"
      // "args": [
      //   "--local-engine",
      //   "host_debug_unopt",
      // ]
    },
    {
      "name": "Dashboard Windows",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "windows"
      // "args": [
      //   "--local-engine",
      //   "host_debug_unopt",
      // ]
    },
    {
      "name": "Dashboard Linux (MAS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "linux",
      "toolArgs": [
        "--dart-define",
        "BUILD=mas"
      ]
    },
    {
      "name": "Dashboard Mac (MAS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "deviceId": "macos",
      "toolArgs": [
        "--dart-define",
        "BUILD=mas"
      ]
    },
    {
      "name": "Dashboard Profile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "deviceId": "linux"
    },
    {
      "name": "Web App",
      "request": "launch",
      "deviceId": "chrome",
      "type": "dart",
      "program": "web_app/lib/main.dart",
      "flutterMode": "debug"
    },
    {
      "name": "Web App (MAS)",
      "request": "launch",
      "deviceId": "chrome",
      "type": "dart",
      "program": "web_app/lib/main.dart",
      "flutterMode": "debug",
      "toolArgs": [
        "--dart-define",
        "BUILD=mas"
      ]
    },
    {
      "name": "Web App Profile",
      "request": "launch",
      "deviceId": "chrome",
      "type": "dart",
      "program": "web_app/lib/main.dart",
      "flutterMode": "profile"
    }
  ]
}